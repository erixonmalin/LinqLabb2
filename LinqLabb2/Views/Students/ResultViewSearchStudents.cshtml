@model IEnumerable<LinqLabb2.Models.Domain.Student>

@{
    ViewData["Title"] = "SearchStudent";
}

<h1>Sökresultat för studenter</h1>

<table class="table table-success table-striped table-hover">
    <thead class="table"><br />
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastName)
                </td>
            </tr>
        }
    </tbody>
</table>
<div>
    <a asp-action="SearchStudents">Ny sökning</a>
</div>
<div>
    <a asp-action="Index">Se alla elever</a>
</div>

